#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>

static unsigned int count_log_F=0;

int log(char data[16],char comment[126], ...){
count_log_F++;
time_t timer;
struct tm *t_st;
FILE *fp=fopen("log.txt","a+");
time(&timer); /*get time*/
fprintf(fp,"data:%s\n#time %s%s\n",data,ctime(&timer),comment);
fclose(fp);
}

int main(int argc,char *argv[]){
log("boot","=NONE=");
}
